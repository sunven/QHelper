{
  "manifest_version": 3,

  "name": "fe-helper",
  "description": "fe-helper",
  "version": "1.0",

  "options_ui": {
    "page": "options.html"
  },

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      // "matches": ["https://unpkg.com/*"],
      "matches": ["<all_urls>"],
      "js": ["js/content_script.js"],
      "run_at": "document_end"
    }
  ],

  // "background": {
  //   "service_worker": "js/background.js"
  // },

  "permissions": ["storage"],

  "host_permissions": ["<all_urls>"]
}
